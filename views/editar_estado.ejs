<%- include('includes/head.ejs') %>
    <%- include('includes/nav.ejs') %>
    <h3 style="text-align: center;"><%= titulo %></h3>
    <br>
    <div class="row">
        <div class="col s12 m11 l11 offset-l1 offset-m1" style="text-align: center;">
            <form action="/zombies/add" method="PUT">
                <label for="estados">Editar Estado </label>
                <select name="estados" id="estados">
                    <option disabled selected>Seleccione un Estado...</option>
                    <% if (lista_estados.length> 0) { %>
                        <% for (let estado of lista_estados) { %>
                            <option value="<%= lugar.id%>"><%= estado.descripcion%></option>
                        <%}%>
                    <%}%> 
                </select>
                <a class="waves-effect waves-light btn" id="button">
                    <input type="submit" id="enviar" name="enviar" value="Enviar">
                </a>
            </form>
        </div>
    </div>
</main>
<script type="text/javascript" src="/js/materialize.min.js"></script>
<script>M.AutoInit();</script>
</body>
